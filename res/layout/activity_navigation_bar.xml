                android:text="@string/dosa"
                android:textColor="#453E3E"
                android:textSize="20sp" />

            <ImageButton
                android:id="@+id/dosa"
                android:layout_width="379dp"
                android:layout_height="186dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="5dp"
                android:background="@drawable/dosa" />
            <TextView
                android:id="@+id/textView13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:fontFamily="sans-serif-black"
                android:gravity="center"
                android:text="@string/aalu"
                android:textColor="#453E3E"
                android:textSize="20sp" />

            <ImageButton
                android:id="@+id/parotha"
                android:layout_width="380dp"
                android:layout_height="186dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="5dp"
                android:background="@drawable/parotha" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:fontFamily="sans-serif-black"
                android:gravity="center"
                android:text="@string/gupcup_panipuri"
                android:textColor="#453E3E"
                android:textSize="20sp" />

            <ImageButton
                android:id="@+id/panipuri"
                android:layout_width="wrap_content"
                android:layout_height="186dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
